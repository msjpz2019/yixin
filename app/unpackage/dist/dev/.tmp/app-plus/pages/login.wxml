<view class="{{['loginView',theme]}}"><my-header vue-id="1" back="{{false}}" menuStatus="{{false}}" bind:__l="__l"></my-header><image class="logo" src="../static/img/common/logo.jpg" mode="aspectFit"></image><view class="content"><view class="loginCell"><input type="text" maxlength="11" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['login']]]]]}}" value="{{login.username}}" bindinput="__e"/></view><view class="loginCell"><input type="password" placeholder="请输入登录密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['login']]]]]}}" value="{{login.password}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['defaultHandlerLogin',['$event']]]]]}}" class="loginBtn" bindtap="__e">登 录</view><view class="loginExtend"><navigator url="#"><text>忘记密码？</text><text class="btn">点击找回</text></navigator></view></view></view>